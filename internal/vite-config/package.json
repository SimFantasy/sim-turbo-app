		"unplugin-vue-components": "catalog:",
		"vite": "catalog:",
		"vite-plugin-compression": "catalog:",
		"vite-plugin-image-optimizer": "catalog:",
		"vite-plugin-mock": "catalog:",
		"vite-plugin-progress": "catalog:",
		"vite-plugin-vue-devtools": "catalog:",
		"vite-plugin-vue-setup-extend": "catalog:",
		"vite-svg-loader": "catalog:"
	},
	"devDependencies": {
		"@sim/typescript-config": "workspace:*",
		"@types/node": "catalog:",
		"eslint": "catalog:",
		"tsup": "catalog:",
		"typescript": "catalog:"
	}
