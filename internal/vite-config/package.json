{
	"name": "@sim/vite-config",
	"type": "module",
	"version": "0.0.1",
	"description": "Vite 公共配置包",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./base": {
			"types": "./dist/base.d.ts",
			"import": "./dist/base.js"
		},
		"./vue": {
			"types": "./dist/vue.d.ts",
			"import": "./dist/vue.js"
		}
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
