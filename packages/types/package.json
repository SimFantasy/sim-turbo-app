{
  "name": "@sim/types",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "main": "./dist/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.ts"
    },
    "./global": {
      "types": "./src/global.d.ts"
    },
    "./router": {
      "types": "./dist/router.d.ts"
    },
    "./service": {
      "types": "./dist/service.d.ts"
    },
    "./vite": {
      "types": "./dist/vite.d.ts"
    }
  },
  "devDependencies": {
    "@sim/tsup-config": "workspace:*",
    "@sim/typescript-config": "workspace:*",
    "axios": "catalog:",
    "eslint": "catalog:",
    "naive-ui": "catalog:",
    "typescript": "catalog:",
    "vue": "catalog:",
    "vue-router": "catalog:"
  }
}
