{
  "name": "@sim/utils",
  "version": "1.0.0",
  "description": "公共工具包",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./storage": {
      "types": "./dist/strorage.d.ts",
      "import": "./dist/storage.js"
    },
    "./array-tree": {
      "types": "./dist/array-tree.d.ts",
      "import": "./dist/array-tree.js"
    },
    "./naive": {
      "types": "./dist/naive.d.ts",
      "import": "./dist/naive.js"
    },
    "./icon": {
      "types": "./dist/icon.d.ts",
      "import": "./dist/icon.js"
    }
  },
  "dependencies": {
    "@iconify/vue": "catalog:",
    "@sim/constants": "workspace:*",
    "@sim/types": "workspace:*",
    "axios": "catalog:",
    "es-toolkit": "catalog:",
    "naive-ui": "catalog:",
    "qs": "catalog:",
    "vue": "catalog:",
    "vue-router": "catalog:"
  },
  "devDependencies": {
    "@sim/eslint-config": "workspace:*",
    "@sim/tsup-config": "workspace:*",
    "@sim/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "@types/qs": "catalog:",
    "eslint": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:"
  }
}
